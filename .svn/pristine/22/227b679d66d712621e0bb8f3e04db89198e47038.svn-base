span,em{
    color: #969696;
}
ul>li{
    background-color: #ffffff;
}
li>div{
   /* height: 50px;*/
    border-bottom: 1px #CAC6B4 solid;
    margin:0 10px;
}
li:first-child>div{
    margin-top: 10px;
}

.mc_one{
    min-height: 100px;
    padding-bottom: 10px;
}

.mc_one>div:first-child{
    height: 40px;
}
.mc_one>div:first-child>img{
    width: 30px;
    border-radius: 15px;
    float: left;
    margin-top: 5px;
}
.mc_one>div:first-child span:nth-child(2){
    float: left;
    line-height: 40px;
    margin-left: 10px;
}
.mc_one>div:first-child span:nth-child(3){
    float: right;
    line-height: 40px;
}
.mc_one>div:nth-child(3){
    width: 50px;
    float: right;
    text-align: center;
    height: 25px;
    border-radius: 9px;
    border: 1px #FCAF00 solid;
}
.mc_one>div:nth-child(3)>span{
    line-height: 25px;
}

.mc_two{
    height: 110px;
}
.mc_two>div:first-child{
    padding: 3px 0;
}
.mc_two>div:first-child>span{
    font-size: 16px;
    color: #000000;
}
.mc_two>div:nth-child(2)>img{
    width: 70px;
}
.mc_two>div:nth-child(3){
    width: 150px;
    float: right;
    text-align: right;
}

.mc_thr{
    padding-top: 10px;
    min-height: 80px;
    border-bottom: 10px;
}
.mc_thr>div{
    float: left;
}
.mc_thr>div:nth-child(1){
    width: 20%;
}
.mc_thr>div:nth-child(1)>img{
    width: 60px;
    height: 60px;
    border-radius: 5px;
}
.mc_thr>div:nth-child(2){
    width: 60%;
    padding-left: 5px;
}
.mc_thr>div:nth-child(2)>span:nth-child(1){
    display: block;
    font-size: 15px;
    line-height: 35px;
    color: #000000;
}
.mc_thr>div:nth-child(2)>span:nth-child(2){
    display: block;
    color: #000000;
}
.mc_thr>div:nth-child(3){
    border: 1px #23c8bf solid;
    float: right;
    width: 50px;
    height: 23px;
    border-radius: 12px;
    text-align: center;
}
.mc_thr>div:nth-child(3)>span{
    line-height: 23px;
}